    var cur_date=0,js_date=0,diff_date=0;
    cur_date={/literal}{$smarty.now}{literal};
    js_date=(new Date().getTime())/ 1000;
    diff_date=Math.floor(js_date-cur_date);
    $("#menu_ongoing").addClass("active");
