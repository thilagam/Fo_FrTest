var mtype='{/literal}{$smarty.get.mission_type}{literal}';
var mid='{/literal}{$smarty.get.mission_identifier}{literal}';

if(mtype && mid )
{
	$.get('/contrib/article-details?req_from=ongoing&misson_type='+mtype+'&mission_identifier='+mid, function(data) {
		$("#viewOffer-ajax .modal-body").html(data);
		$("#viewOffer-ajax").modal('show');
	});
}
$(document).ready(function(){
    //call this funcion to load Recent Ao offers on the page using ajax//
    loadOngoingAjaxRecentAoOffers();
    //call tp load Published Articles//
    loadOngoingAjaxPublishedArticles();
    loadOngoingAjaxEncoursArticles();

    loadOngoingAjaxAwaitingArticles();
});
function loadOngoingAjaxRecentAoOffers(){
    console.log('initiated loadOngoingAjaxRecentAoOffers');
    $.ajax({
        type: 'POST',
        url: '/contrib/load-ongoing-ajax-recent-ao-offers',
        success: function (data) {
            $("#recent_AO_Offers").html(data);
        }
    });
}
function loadOngoingAjaxPublishedArticles(){
    console.log('initiated loadOngoingAjaxPublishedArticles');
    $.ajax({
        type: 'POST',
        url: '/contrib/load-ongoing-ajax-published-articles',
        success: function (data) {
            $("#previous-work").html(data);
        }
    });
}
function loadOngoingAjaxEncoursArticles(){
    console.log('initiated loadOngoingAjaxEncoursArticles');
    $.ajax({
        type: 'POST',
        url: '/contrib/load-ongoing-ajax-encours-articles',
        success: function (data) {
            $("#ongoing-stack").html(data);
        }
    });
}

function loadOngoingAjaxAwaitingArticles(){
	console.log('initiated loadOngoingAjaxAwaitingArticles');
    $.ajax({
        type: 'POST',
        url: '/contrib/load-ongoing-ajax-awaiting-articles',
        success: function (data) {
            $("#awaiting_Articles").html(data);
        }
    });
}

