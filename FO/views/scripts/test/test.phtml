	{else}
	<li><a href="#{$key1|capitalize}" data-toggle="tab">{$key1|capitalize}</a></li>
	{/if}		
	{if $smarty.foreach.typeusers.last}
	</ul>
	{/if}
{/foreach}
{foreach from=$req_users item=item1 name=typeusers key=key1}
			{if $smarty.foreach.typeusers.first}
		<div class="tab-content">
			<div class="tab-pane active" id="{$key1|capitalize}">
			{else}
			<div class="tab-pane" id="{$key1|capitalize}">
			{/if}
